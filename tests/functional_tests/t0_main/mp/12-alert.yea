id: 0.mp.12-alert
plugin:
  - wandb
  
assert:
  - :wandb:runs_len: 1
  - :wandb:runs[0][config]: {}
  - :wandb:runs[0][summary]:
      m1: 1
      m2: 2
  - :wandb:runs[0][exitcode]: 0
  - :op:contains:
    - :wandb:runs[0][telemetry][3]  # feature
    - 23  # service
  - :wandb:runs[0][alerts][0]:
      severity: ERROR
      text: and full text
      title: this-is-my-title
      wait: 60000
