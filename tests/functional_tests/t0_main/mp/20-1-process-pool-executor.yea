id: 0.mp.20-1-process-pool-executor  

tag:
  skips:
    - platform: win

var:
  - history_len:
      :fn:len: :wandb:runs[0][history]

parametrize:
    permute:
        - :yea:start_method:
            - spawn
            - forkserver
            - fork
plugin:
  - wandb

assert:
  - :wandb:runs_len: 1
  - :history_len: 3
  - :wandb:runs[0][exitcode]: 0
