id: 0.mp.10-metric
plugin:
  - wandb
tag:
  shard: service
assert:
  - :wandb:runs_len: 1
  - :wandb:runs[0][config]: {}
  - :wandb:runs[0][summary]:
      m1: 1
      m2:
        max: 8
  - :wandb:runs[0][exitcode]: 0
  - :op:contains:
    - :wandb:runs[0][telemetry][3]  # feature
    - 23  # service
  - :op:contains:
    - :wandb:runs[0][telemetry][3]  # feature
    - 7  # define_metric
