    - 53  # joblib

  - :op:contains:
    - :run_1[telemetry][3]  # feature
    - 23  # service

  - :op:contains:
    - :run_1[telemetry][2]  # import init
    - 53  # joblib

  - :op:contains:
    - :run_2[telemetry][3]  # feature
    - 23  # service

  - :op:contains:
    - :run_2[telemetry][2]  # import init
    - 53  # joblib

  - :op:contains:
    - :run_2[telemetry][3]  # feature
    - 23  # service

  - :op:contains:
    - :run_3[telemetry][2]  # import init
    - 53  # joblib


  - :run_0[exitcode]: 0
  - :run_1[exitcode]: 0
  - :run_2[exitcode]: 0
  - :run_3[exitcode]: 0
