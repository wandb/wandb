id: 0.mp.21-1-joblib-parallel
tag:
  shard: service

plugin:
  - wandb

depend:
  requirements:
    - joblib

var:
  - run_0:
      :fn:find:
      - item
      - :wandb:runs
      - :item[config][x]: 0

  - run_1:
      :fn:find:
      - item
      - :wandb:runs
      - :item[config][x]: 1

  - run_2:
      :fn:find:
      - item
      - :wandb:runs
      - :item[config][x]: 4

  - run_3:
