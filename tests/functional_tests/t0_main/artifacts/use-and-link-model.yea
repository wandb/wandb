id: 0.artifacts.use-and-link-model
plugin:
  - wandb
tag:
  shards:
    - artifacts
depend:
  requirements:
    - cloudpickle
    - torch
assert:
  - :wandb:runs_len: 1
  - :wandb:runs[0][config]: {}
  - :wandb:runs[0][exitcode]: 0
  - :wandb:artifacts[my-model][type]: model
  - :wandb:portfolio_links[test_portfolio][num]: 1