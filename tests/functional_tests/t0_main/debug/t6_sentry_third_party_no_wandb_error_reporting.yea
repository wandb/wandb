id: 0.debug.06-sentry-third-party-no-wandb-error-reporting
plugin:
  - wandb
tag:
  platforms:
    - linux
    - mac
command:
  program: sentry_third_party.py
depend:
  requirements:
    - sentry_sdk
env:
  - WANDB_ERROR_REPORTING: "false"
var:
  - num_sentry_events:
      :fn:len: :wandb:sentry_events
assert:
  - :wandb:runs_len: 1
  - :num_sentry_events: 0
  - :wandb:runs[0][summary][num_third_party_sentry_events]: 2
  - :yea:exit: 255
