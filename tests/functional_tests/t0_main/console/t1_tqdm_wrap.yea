id: 0.console.t1_tqdm_wrap
plugin:
  - wandb
depend:
  requirements:
    - tqdm
command:
  program: test_tqdm.py
tag:
  skips:
    - platform: win
env:
  - WANDB_CONSOLE: wrap
assert:
  - :wandb:runs_len: 1
  - :wandb:runs[0][config]: {}
  - :wandb:runs[0][summary]:
      this: 2
  - :wandb:runs[0][exitcode]: 0
  - :op:contains_regex:
    - :wandb:runs[0][output][stderr]
    - after progress 
  - :op:contains_regex:
    - :wandb:runs[0][output][stdout]
    - final progress
