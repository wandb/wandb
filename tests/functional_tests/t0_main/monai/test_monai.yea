id: 0.monai.segmentation
plugin:
    - wandb
tag:
  platforms:
    - linux
command:
    program: test_monai.py
depend:
    requirements:
        - "monai>=1.3.0"
        - "pytorch-ignite>=0.4.11"
        - "nibabel>=5.2.0"
assert:
    - :wandb:runs_len: 1
    - :wandb:runs[0][config][epochs]: 2
    - :wandb:runs[0][config][train_batch_size]: 2
    - :wandb:runs[0][config][val_batch_size]: 5
    - :op:>=:
        - :wandb:runs[0][summary][epoch/Mean_Dice]
        - 0
    - :op:==:
        - :wandb:runs[0][summary][epoch/epoch]
        - 2
    - :op:>=:
        - :wandb:runs[0][summary][iteration/Loss]
        - 0
    - :op:==:
        - :wandb:runs[0][summary][iteration/iteration]
        - 20
    - :wandb:runs[0][exitcode]: 0
