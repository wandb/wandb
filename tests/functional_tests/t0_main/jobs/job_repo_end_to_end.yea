id: 4.jobs.repo-end-to-end
plugin:
  - wandb
tag:
  platforms:
    - linux
  shards:
    - launch
    - wandb_core_failure
depend:
  requirements:
    - pytest
assert:
  - :wandb:runs_len: 1
  - :wandb:runs[0][exitcode]: 0
  - :wandb:artifacts[job-git_github.com_wandb_wandb.git_tests_functional_tests_t0_main_jobs_job_repo_creation.py][type]: job
  - :yea:exit: 0
