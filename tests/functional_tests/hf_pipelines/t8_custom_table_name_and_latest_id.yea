id: 0.llm-hf.01-text-classification
plugin:
  - wandb
tag:
  shard: llm
#  skip: true  # todo: fix
  skips:
    - platform: win
depend:
  requirements:
    - "transformers"
    - "torch"
assert:
    - :wandb:runs_len: 1
    - :wandb:runs[0][exitcode]: 0
    - :wandb:runs[0][summary][custom_table][_type]: table-file
    - :wandb:runs[0][summary][custom_table][ncols]: 7
    - :wandb:runs[0][summary][custom_table][nrows]: 1