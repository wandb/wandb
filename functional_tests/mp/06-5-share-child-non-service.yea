id: 0.mp.06-5-share-child-non-service
plugin:
  - wandb
parametrize:
  permute:
    - :yea:start_method:
      - spawn
      - fork
var:
  - history_0_len:
      :fn:len: :wandb:runs[0][history]

assert:
  - :wandb:runs_len: 1
  - :wandb:runs[0][config][c1]: 11 
  - :wandb:runs[0][history][0][s1]: 11
  - :wandb:runs[0][history][0][_step]: 0
  - :wandb:runs[0][summary][s1]: 11
  - :wandb:runs[0][exitcode]: 0
