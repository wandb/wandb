id: init-and-log-image-pillow
plugin:
    - wandb
tag:
  shard: docs
depend:
    requirements:
        - numpy
        - pillow
assert:
    - :wandb:runs_len: 1
    - :op:contains:
      - :wandb:runs[0][summary]
      - examples
    - :wandb:runs[0][exitcode]: 0
    - :wandb:runs[0][summary][examples]:
        _type: images/separated
        width: 100
        height: 100
        format: png
        count: 3
        captions:
          - random field 0
          - random field 1
          - random field 2
