id: 0.llm-openai.01-edit
plugin:
  - wandb
tag:
  shard: llm
  skips:
    - platform: win
depend:
  requirements:
    - "openai<=0.28.1"
assert:
    - :wandb:runs_len: 1
    - :wandb:runs[0][exitcode]: 0
    - :wandb:runs[0][history][0][trace][_type]: wb_trace_tree
    - :wandb:runs[0][summary][stats][_type]: table-file
    - :wandb:runs[0][summary][stats][nrows]: 1
    - :wandb:runs[0][summary][stats][ncols]: 12
    - :op:contains:
        - :wandb:runs[0][history][0][trace]
        - root_span_dumps
    - :op:contains:
        - :wandb:runs[0][summary]
        - usage/completion_tokens
    - :op:contains:
        - :wandb:runs[0][summary]
        - usage/prompt_tokens
    - :op:contains:
        - :wandb:runs[0][summary]
        - usage/total_tokens
    - :op:contains:
        - :wandb:runs[0][history][0][trace]
        - model_dict_dumps
