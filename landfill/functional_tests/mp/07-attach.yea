id: 0.mp.07-attach
plugin:
  - wandb
tag:
  skips:
    - platform: win
parametrize:
  permute:
    - :yea:start_method:
      - spawn
      - forkserver
assert:
    - :wandb:runs_len: 1
    - :wandb:runs[0][summary][s1]: 21
    - :wandb:runs[0][summary][s2]: 22
    - :wandb:runs[0][summary][s3]: 13
    - :wandb:runs[0][summary][s4]: 14
    - :wandb:runs[0][config][c1]: 11
    - :wandb:runs[0][config][c2]: 22
    - :op:contains:
      - :wandb:runs[0][telemetry][3]  # feature
      - 23  # service
    - :op:contains:
      - :wandb:runs[0][telemetry][3]  # feature
      - 33  # attach
